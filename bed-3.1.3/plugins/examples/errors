clang++  -DNOEDITOR -DGLOBAL -DUNDER -fPIC  -I../ -I../../src/ -I../../src/empty     -DNEWTERM     -DBEDSRCDIR=../.. -D__ANDROID_API__=21 -idirafter /home/jka/Android/arm64d/sysroot/usr/include/linux -std=c++17 -Wall    -DCURSESTYPE=ncurses -DCURSES=1 -DTEMPDIR=\"/data/data/com.termux/files/usr/tmp\" -DPERSCONFFILE=\"/sdcard/.bedrc\"  -DDEFAULTBEDRCBIN=\"/sdcard/.bedrc-bin\" -DGLOBALCONFFILE=\"/data/data/com.termux/files/usr/share/bed/bedrc\"  -DHAVE_CONFIG_H   -imacros ../defines.h  -DVERSION=\"3.0.1\" -DPROGRAM=\"bed\"  -DCONFDIR=\"/data/data/com.termux/files/usr/lib64/bed-3.0.1\" -DCONFDIRLINK=\"/data/data/com.termux/files/usr/share/bed\" -DBINDIR=\"/data/data/com.termux/files/usr/bin\" -DCONFVAR=\"BEDRC\"  -DCONFFILE=\"bedrc\"  -DLARGEFILES=1 -DUSETHREADS=1  -D_REENTRANT=1  -O3    -DPLUGIN -DEXAMPLEDIR=\"../examples\" -DSOURCEFILENAME=\"examples/bitfields.cc\" -c bitfields.cc -o bitfields.o
clang++  -O3 -s    -Wl,-z,noexecstack   -shared  bitfields.o bitcpy.o -o bitfields.plug 
